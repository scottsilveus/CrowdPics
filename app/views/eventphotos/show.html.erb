<p id="notice"><%= notice %></p>

<h2><%= @eventphotos.name %></h2>
  <%= image_tag @eventphoto.avatar.url(:medium) %>

<div class="show actions">
  <%= link_to 'Edit', edit_eventphoto_path(@eventphotos) %>
  <%= link_to 'Remove', @eventphotos, data: { confirm: 'Are you sure you want to delete this event photo?' }, method: :delete, class: 'destroy' %>
  <%= link_to 'Back to all my event photos', eventphotos_path %>
</div>