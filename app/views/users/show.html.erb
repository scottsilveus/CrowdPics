<h1>Show page</h1>

<ol>
<% current_user.events.order(:start_date).each do |event| %>
	<li><%= link_to "#{event.title}", event_path(event) %> Date: <%= event.start_date.strftime("%m/%d/%y") %></li>
<% end %>
</ol>
<%= link_to "Create Event", new_event_path %>
<%= link_to "LogOut", destroy_user_session_path, method: :delete %>
<%= link_to "Delete", edit_user_registration_path, method: :delete, confirm: "Are you sure?" %>
